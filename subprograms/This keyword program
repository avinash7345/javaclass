//Employee class//

package com.cts.vms;

public class Employee {
int empno;
String ename;
int sal;
String type;

Employee (){
	 empno = 10;
	 ename = "abc";
	 sal = 50000;
     type = "permanent";

}
Employee (int empno,String ename,int sal,String type){
	this.empno = empno;
	this.ename = ename;
	this.sal = sal;
	this.type = type;
	
}
void displayMethod() {
	System.out.println("emp name is"+ename+"sal is"+sal);
}

}

//Employeeex class//
package com.cts.vms;

public class ExEmployee {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Employee e1 = new Employee();
Employee e2 = new Employee();
e1.displayMethod();
e2.displayMethod();
Employee e3 = new Employee(10,"20",30,"40");
Employee e4 = new Employee(50,"60",70,"80");

e1.displayMethod();
e2.displayMethod();
e3.displayMethod();
e4.displayMethod();

	}

}
